{
    "name": "tp-bloomland/snapcapture-client",
    "description": "SnapCapture API client for WordPress plugin",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "SnapCapture\\": "includes/SnapCapture/",
            "TrafficPortal\\": "includes/TrafficPortal/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/php/"
        }
    }
}
